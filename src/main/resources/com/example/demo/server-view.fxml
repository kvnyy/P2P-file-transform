<BorderPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo.ServerController">
   <top>
      <Label fx:id="welcomeText" alignment="CENTER" opacity="0.88" text="服务器" textAlignment="CENTER" textFill="#282822" BorderPane.alignment="CENTER">
         <font>
            <Font name="Arial" size="24.0" />
         </font>
      </Label>
   </top>
   <bottom>
      <HBox prefHeight="100.0" prefWidth="200.0">
         <children>
            <Button fx:id="showFile" mnemonicParsing="false" onAction="#onshowfileButtonCilck" prefHeight="46.0" prefWidth="113.0" text="显示文件" translateX="49.0" translateY="25.0">
               <font>
                  <Font size="20.0" />
               </font></Button>
            <Button fx:id="startButton" mnemonicParsing="false" onAction="#onstartButtonClick" prefHeight="42.0" prefWidth="95.0" text="启动" translateX="132.0" translateY="25.0">
               <font>
                  <Font size="20.0" />
               </font></Button>
            <Button fx:id="endButton" mnemonicParsing="false" onAction="#onendButtonCilck" prefHeight="38.0" prefWidth="94.0" text="结束" translateX="232.0" translateY="25.0">
